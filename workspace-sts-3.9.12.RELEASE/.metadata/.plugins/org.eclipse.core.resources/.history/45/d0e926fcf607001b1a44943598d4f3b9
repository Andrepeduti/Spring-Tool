package org.generation.blogPessoal.repository;

import java.util.List;

import org.generation.blogPessoal.controller.TemaController;
import org.springframework.data.jpa.repository.JpaRepository;

public interface temaRepository extends JpaRepository<TemaController, Long >{
	
	public List<Tema> findAllByDescricaoContainingCase(String descricao);

}
